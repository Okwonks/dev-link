<div class="navbar-fixed">
  <!-- this will give the navbar the fixed property -->
  <nav>
    <div class="nav-wrapper">
      {{#link-to 'index' class="brand-logo center"}}<img src="http://i.imgur.com/rG21AQM.png" alt="Dev-Link">{{/link-to}}
      <a href="#" data-activates="mobile-demo" class="button-collapse" {{action 'mobile'}}><i class="material-icons">menu</i></a>
      <ul id="nav-mobile" class="left hide-on-med-and-down">
        <li>{{#link-to 'index'}}<i class="material-icons">home</i>{{/link-to}}</li>
        <li>{{#link-to 'about'}}About{{/link-to}}</li>
        <li>{{#link-to 'forum'}}Forum{{/link-to}}</li>
        <li>{{#link-to 'blog'}}Blog{{/link-to}}</li>
        <li>{{#link-to 'contact'}}Contact Us{{/link-to}}</li>
      </ul>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
        {{#if session.isAuthenticated}}
        <button class="waves-effect waves-light btn" {{action 'logout'}}>Logout</button> {{/if}}
        <li>{{#link-to 'developer'}}Developers{{/link-to}}</li>
        <li>{{#link-to 'languages'}}Languages{{/link-to}}</li>
        <li>{{#link-to 'signup' signUp=signUp login=login}}Sign-up{{/link-to}}</li>
        <li>{{#link-to 'login' signUp=signUp login="login"}}Login{{/link-to}}</li>
      </ul>
      <ul class="side-nav" id="mobile-demo">
        <li>{{#link-to 'index'}}<i class="material-icons">home</i>{{/link-to}}</li>
        <li>{{#link-to 'about'}}About{{/link-to}}</li>
        <li>{{#link-to 'forum'}}Forum{{/link-to}}</li>
        <li>{{#link-to 'blog'}}Blog{{/link-to}}</li>
        <li>{{#link-to 'contact'}}Contact Us{{/link-to}}</li>
        {{#if session.isAuthenticated}}
        <button class="waves-effect waves-light btn" {{action 'logout'}}>Logout</button> {{/if}}
        <li>{{#link-to 'developer'}}Developers{{/link-to}}</li>
        <li>{{#link-to 'languages'}}Languages{{/link-to}}</li>
        <li>{{#link-to 'signup' signUp=signUp login=login}}Sign-up{{/link-to}}</li>
        <li>{{#link-to 'login' signUp=signUp login="login"}}Login{{/link-to}}</li>
      </ul>
    </div>
  </nav>
</div>

<div class="container-fluid">
  {{outlet}}
</div>
{{routable-modal-outlet}}
<br>
<br>
<div class="container">
  <hr>
</div>


